local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "LazeL---UNIVERSAL", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


OrionLib:MakeNotification({
	Name = "LazeL",
	Content = "Thank You For Using LazeL",
	Image = "rbxassetid://4483345998",
	Time = 7
})


OrionLib:MakeNotification({
	Name = "LazeL",
	Content = "Join Our Discord For Any Help!",
	Image = "rbxassetid://4483345998",
	Time = 10
})


